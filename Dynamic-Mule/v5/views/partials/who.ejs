<!-- Who We Are -->
<div id="whoWeAre" class="row justify-content-center align-items-center p-5 mt-n3">
    <!-- chris -->
    <% users.forEach(function (user, i) { %>
        <% if (user.display) { %>
            <% let j = num => num % 2; %>
                <div class="row justify-content-center align-items-center mb-4">
                    <div class="col-lg-5 col-12 content text-left p-2 lead order-<%= j(i)+1 %>">
                        <h2 class="border-bottom border-info">
                            <%= user.name %>
                        </h2>
                        <%- user.bio %>
                    </div>
                    <div class="col-lg-5 col-10 order-<%= j(i)+1 %> order-lg-1">
                        <img class=" img-fluid border border-info mx-auto shadow-sm about-image " src="<%=user.image.secure_url %>">
                    </div>
                </div>
                <% }}) %>
