<%- include("partials/header") %>

    <!-- <%- // include("partials/logobar") %> -->

    <div class="container-fluid mt-5">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/projects">Projects</a></li>
                <li class="breadcrumb-item">
                    <a href="/projects/#<%= project.type %>Header">
                        <%= project.type %>
                    </a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">
                    <%= project.title %>
                </li>
            </ol>
        </nav>
        <div class="display-4 text-center pb-2">
            <%= project.title %>
        </div>
        <!-- image grid -->

        <!-- Main Image -->
        <div class="row justify-content-center">
            <div class="col-10">
                <img src="<%= project.image %>" class="img-fluid border mx-auto d-block" alt="...">
            </div>
        </div>

        <!-- Row of 4 additional images -->
        <div class="row align-items-center mt-3 py-3 border-bottom border-top border-secondary">
            <div class="col-6 col-md-3" data-toggle="modal" data-target="#image1">
                <img src="<%= project.image1 %>" class="img-thumbnail img-fluid border shadow-sm" alt="...">
            </div>
            <div class="col-6 col-md-3" data-toggle="modal" data-target="#image2">
                <img src="<%= project.image2 %>" class="img-thumbnail img-fluid border shadow-sm" alt="...">
            </div>
            <div class="col-6 col-md-3" data-toggle="modal" data-target="#image3">
                <img src="<%= project.image3 %>" class="img-thumbnail img-fluid border shadow-sm" alt="...">
            </div>
            <div class="col-6 col-md-3" data-toggle="modal" data-target="#image4">
                <img src="<%= project.image4 %>" class="img-thumbnail img-fluid border shadow-sm" alt="...">
            </div>
        </div>

        <!-- Modals -->
        <div class="modal fade" id="image1" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true">
            <div class="modal-dialog modal-lg" data-dismiss="modal" aria-label="Close">
                <div class="modal-content">
                    <div class="modal-body">
                        <img src="<%= project.image1 %>" class="img-fluid border shadow-sm" alt="...">
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="image2" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true">
            <div class="modal-dialog modal-lg" data-dismiss="modal" aria-label="Close">
                <div class="modal-content">
                    <div class="modal-body">
                        <img src="<%= project.image2 %>" class="img-fluid border shadow-sm" alt="...">
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="image3" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true">
            <div class="modal-dialog modal-lg" data-dismiss="modal" aria-label="Close">
                <div class="modal-content">
                    <div class="modal-body">
                        <img src="<%= project.image3 %>" class="img-fluid border shadow-sm" alt="...">
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="image4" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true">
            <div class="modal-dialog modal-lg" data-dismiss="modal" aria-label="Close">
                <div class="modal-content">
                    <div class="modal-body">
                        <img src="<%= project.image4 %>" class="img-fluid border shadow-sm" alt="...">
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="lead my-3">
                <div class="lead">
                    <p>
                        <%= project.description %>
                    </p>
                    <p>
                        <%= project.details %>
                    </p>
                    <p>
                        This project was completed with the help of <strong>
                    <%= project.collaborators %></strong>.
                    </p>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="card mb-3" style="max-width: 800px;">
                    <div class="row no-gutters">
                        <div class="col-md-5">
                            <iframe class="card-img border-right" width="350" height="300" frameborder="0" src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCtu_gSxLrO9DpwsKSeE9HnZpDRnA-ZXfo&q=<%= project.projAddressStreet.split(' ').join('+'); %>,<%= project.projAddressCity%>,<%= project.projAddressState%>">
                    </iframe>
                        </div>
                        <div class="col-md-7 mt-auto">
                            <div class="card-body pb-5">
                                <h5 class="card-title">Client Info</h5>
                                <p class="card-text">This project was completed for
                                    <%= project.clientName%>. See our work in person or visit them online at
                                        <a href="http://<%= project.clientWebsite %>">
                                            <%= project.clientWebsite %>
                                        </a>
                                </p>
                            </div>
                            <div class="card-footer">
                                <blockquote class="blockquote">
                                    <p class="mb-0">
                                        <%= project.customerReview%>
                                    </p>
                                    <footer class="blockquote-footer"><cite title="Source Title"><%=project.clientName%></cite></footer>
                                </blockquote>
                            </div>
                            <!-- <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p> -->
                        </div>
                    </div>
                </div>
            </div>
        </div>



    </div>

    <!-- <a href="/projects/<%= project._id %>/edit" class="btn btn-warning btn-sm">EDIT</a>
            <form id="delete" action="/projects/<%= project._id %>?_method=DELETE" method="POST">
                <button class="btn btn-sm">Delete</button>
            </form> -->
    </div>

    </div>

    <%- include("partials/footer") %>