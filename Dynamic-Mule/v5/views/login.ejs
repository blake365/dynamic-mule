<%- include("partials/header") %>
    <div class="row justify-content-center pt-5">
        <div class="col-md-6 col-10 my-3">
            <div class="card ">
                <h1 class="card-header">Login</h1>
                <div class="card-body">
                    <form action="/login" method="post">
                        <div class="form-group">
                            <label for="username">Username:</label>
                            <input class="form-control" type="text" name="username" id="username" placeholder="username" required></div>
                        <div class="form-group">
                            <label for="password">Password:</label>
                            <input class="form-control" id="password" type="password" name="password" placeholder="password" required></div>
                        <button type="submit" class="btn btn-lg btn-success">Login</button>

                    </form>
                </div>
            </div>
        </div>
    </div>
    <%- include("partials/footer") %>